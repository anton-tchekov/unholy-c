fn fib(n)
{
	if le(n, 1) {
		return n;
	}

	return add(fib(sub(n, 1)), fib(sub(n, 2)));
}

fn sum(n)
{
	if eq(n, 0) {
		return 0;
	}

	return add(sum(sub(n, 1)), n);
}

fn main()
{
	print_number(100);
	print_number(sum(7));
	print_number(mul(add(100, 42), 69));

	print_number(mul(add(1, 2), sub(7, 5)));

	print_number(fib(15));
	println("Hello World");
	return 0;
}

