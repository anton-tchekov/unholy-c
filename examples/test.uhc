/* ARRAY */
fn array_new(length, elem_size)
{
	var addr;
	addr = alloc(add(mul(length, elem_size), 12));
	w32(addr, length);
	w32(add(addr, 4), elem_size);
	return addr;
}

fn array_access(array, index)
{
	var length, elem_size;
	length = r32(array);
	elem_size = r32(add(array, 4));
	if hs(index, length)
	{
		return 0;
	}

	return add(add(array, 8), mul(index, elem_size));
}

fn array_destroy(array)
{
	free(array);
}

